  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@react-router/dev": "^7.5.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
